<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
  <Button name="BetterBagsSectionConfigListButton" virtual="true">
    <Size>
      <AbsDimension y="35"/>
    </Size>
    <HighlightTexture inherits="UIPanelButtonHighlightTexture"/>
    <Layers>
      <Layer level="OVERLAY">
        <FontString name="$parentRowNumber" inherits="GameFontHighlightLarge" parentKey="RowNumber">
          <Color r="1" g="1" b="1" />
          <Anchors>
            <Anchor point="LEFT"/>
          </Anchors>
          <Size>
            <AbsDimension x="50" y="35"/>
          </Size>
        </FontString>
        <FontString inherits="GameFontNormalLarge" parentKey="Category">
          <Color r="1" g="1" b="1" />
          <Anchors>
            <Anchor point="LEFT" relativeTo="$parentRowNumber" relativePoint="RIGHT"/>
          </Anchors>
          <Size>
            <AbsDimension y="35"/>
          </Size>
        </FontString>
      </Layer>
    </Layers>
  </Button>
</Ui>